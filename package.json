{
  "name": "root",
  "private": true,
  "repository": {
    "type": "git",
    "url": "git+https://github.com/getodk/web-forms.git"
  },
  "type": "module",
  "version": "0.1.0",
  "engines": {
    "node": "^20.19.3 || ^22.17.0 || ^24.3.0",
    "yarn": "1.22.22"
  },
  "volta": {
    "node": "22.17.0",
    "yarn": "1.22.22"
  },
  "packageManager": "yarn@1.22.22",
  "workspaces": [
    "packages/*"
  ],
  "scripts": {
    "preinstall": "npx only-allow yarn",
    "test": "turbo run test",
    "build": "turbo run build",
    "types": "turbo run types",
    "bump": "changeset add",
    "format": "npm-run-all --print-name format:vue format:prettier",
    "format:prettier": "prettier -w \"**/*\" --ignore-unknown --cache",
    "format:vue": "eslint ./**/*.vue  --report-unused-disable-directives --fix",
    "format:readme-only": "prettier -w README.md",
    "format:checkonly": "prettier -c \"**/*\" --ignore-unknown",
    "lint": "eslint . --report-unused-disable-directives",
    "feature-matrix": "node scripts/feature-matrix/render.js"
  },
  "dependencies": {
    "@changesets/changelog-github": "^0.5.1",
    "@changesets/cli": "^2.29.5",
    "@eslint/js": "^9.30.1",
    "@tsconfig/node20": "^20.1.6",
    "@types/eslint": "^9.6.1",
    "@types/eslint-config-prettier": "^6.11.3",
    "@types/eslint__js": "^9.14.0",
    "@types/geojson": "^7946.0.16",
    "@types/jsdom": "^21.1.7",
    "@types/node": "^24.0.11",
    "@typescript-eslint/eslint-plugin": "^8.36.0",
    "@typescript-eslint/parser": "^8.36.0",
    "@vue/eslint-config-prettier": "^10.1.0",
    "@vue/eslint-config-typescript": "^14.6.0",
    "@vue/tsconfig": "^0.7.0",
    "eslint": "^9.30.1",
    "eslint-config-prettier": "^10.1.5",
    "eslint-plugin-jsdoc": "^51.3.4",
    "eslint-plugin-no-only-tests": "^3.3.0",
    "eslint-plugin-prettier": "^5.5.1",
    "eslint-plugin-prettier-vue": "^5.0.0",
    "eslint-plugin-vue": "^10.3.0",
    "npm-run-all2": "^7.0.2",
    "only-allow": "^1.2.1",
    "organize-imports-cli": "^0.10.0",
    "prettier": "^3.6.2",
    "rimraf": "^5.0.9",
    "turbo": "^2.5.4",
    "typescript": "~5.8.3",
    "typescript-eslint": "^8.36.0",
    "vue": "3.5.17",
    "vue-tsc": "^3.0.1"
  },
  "resolutions": {
    "**/tree-sitter": "0.22.1"
  }
}
